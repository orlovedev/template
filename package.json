{
	"name": "template",
	"description": "description",
	"version": "0.0.0",
	"main": "index.js",
	"engines": {
		"node": ">=10.0.0"
	},
	"repository": "https://github.com/priestine/template.git",
	"author": "Sergei Orlov <orlove.dev@gmail.com> (https://orlove.dev/)",
	"keywords": [],
	"license": "MIT",
	"homepage": "https://orlove.dev/repos/template",
	"bugs": {
		"email": "orlove.dev@gmail.com",
		"url": "https://github.com/priestine/template/issues"
	},
	"scripts": {
		"start": "ts-node src/index.ts",
		"build": "tsc",
		"test": "ava",
		"test:cover": "nyc ava",
		"lint:ts": "xo",
		"lint:tsc": "tsc --noEmit",
		"lint": "yarn lint:ts && yarn lint:tsc",
		"prettify": "pretty-quick"
	},
	"dependencies": {},
	"optionalDependencies": {},
	"peerDependencies": {},
	"devDependencies": {
		"@commitlint/cli": "^8.3.5",
		"@types/node": "^14.0.13",
		"ava": "^3.8.2",
		"commitlint-config-gitmoji": "^1.0.1",
		"cross-env": "^7.0.2",
		"eslint-plugin-prettier": "^3.1.3",
		"husky": "^4.2.5",
		"nyc": "^15.1.0",
		"prettier": "^2.0.5",
		"pretty-quick": "^2.0.1",
		"ts-node": "^8.10.2",
		"typescript": "^3.9.5",
		"xo": "^0.32.0",
		"yarn": "^1.22.4"
	},
	"husky": {
		"hooks": {
			"commit-msg": "yarn prettify && commitlint -E HUSKY_GIT_PARAMS",
			"pre-push": "yarn lint && yarn test:cover"
		}
	},
	"xo": {
		"semicolon": false,
		"prettier": true
	},
	"ava": {
		"files": [
			"test/**/*"
		],
		"extensions": [
			"ts"
		],
		"require": [
			"ts-node/register"
		],
		"concurrency": 5,
		"failFast": true,
		"failWithoutAssertions": false,
		"verbose": true,
		"nodeArguments": [
			"--trace-deprecation",
			"--napi-modules"
		]
	},
	"nyc": {
		"reporter": [
			"text",
			"lcov",
			"html"
		],
		"branches": 80,
		"lines": 80,
		"functions": 80,
		"statements": 80,
		"check-coverage": true,
		"all": true
	},
	"commitlint": {
		"extends": [
			"./node_modules/commitlint-config-gitmoji"
		]
	}
}
